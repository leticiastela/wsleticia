package variaveis;

public class ManipulaString {
	public static void main(String[] args) {
		String email = "LeTicIA.STELA@itau-UNIBANCO.com.bR";
		System.out.println("Original:" + email);
		System.out.println("Minúsculo:" + email.toLowerCase());
		System.out.println("Minúsculo:" + email.toUpperCase());
		System.out.println("Minúsculo:" + email.contains("@"));
	}

}
